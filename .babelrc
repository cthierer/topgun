{
  "presets": [
    ["babel-preset-env", {
      "targets": {
        "browsers": ["last 2 versions", "> 1%", "not ie < 11"]
      }
    }],
    ["babel-preset-react"]
  ],
  "plugins": [
    ["babel-plugin-transform-object-rest-spread", { "useBuiltIns": true }],
    ["babel-plugin-transform-builtin-extend", { "globals": ["Error", "Array"] }]
  ]
}
